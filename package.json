{
	"name": "batched-function",
	"version": "2.0.0",
	"description": "Batch multiple function calls into a single one",
	"license": "MIT",
	"repository": "Richienb/batched-function",
	"type": "module",
	"exports": "./dist/index.js",
	"engines": {
		"node": ">=14"
	},
	"scripts": {
		"build": "tsc --build --clean",
		"test": "xo && ava",
		"prepublishOnly": "tsc --build --clean"
	},
	"files": [
		"dist"
	],
	"types": "dist/index.d.ts",
	"keywords": [
		"function",
		"batch",
		"interval"
	],
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"ava": "^5.1.1",
		"p-defer": "^4.0.0",
		"time-span": "^5.1.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5",
		"xo": "^0.53.1"
	},
	"ava": {
		"extensions": {
			"ts": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	}
}
